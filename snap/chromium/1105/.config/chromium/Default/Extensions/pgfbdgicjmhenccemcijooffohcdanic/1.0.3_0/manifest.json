      "matches": [ "*://localhost/*", "*://*.ceb.loc/*", "*://*.p24.ua/*", "*://*.pb.ua/*", "*://*.pb.com.ua/*", "*://*.privat24.ua/*", "*://*.privatbank.ua/*", "*://*.biprocessing.org.ua/*", "*://*.igov.org.ua/*", "*://*.bankid.org.ua/*" ]
   },
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "24": "icon24.png",
      "256": "icon256.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "512": "icon512.png",
      "64": "icon64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "BankID CryptoPlugin",
   "permissions": [ "tabs", "nativeMessaging" ],
